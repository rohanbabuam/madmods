{
	"name": "madmods",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler pages dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"deploy": "pnpm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types && move worker-configuration.d.ts src/"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250327.0",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"@sveltejs/adapter-cloudflare": "^6.0.1",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.17",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/three": "^0.174.0",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.0.17",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.28.0",
		"vite": "^6.2.3",
		"vitest": "^3.0.9",
		"wrangler": "^4.5.1"
	},
	"packageManager": "pnpm@10.6.5",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@babylonjs/core": "^8.0.1",
		"@babylonjs/gui": "^8.0.1",
		"@babylonjs/havok": "^1.3.10",
		"@babylonjs/loaders": "^8.0.1",
		"@babylonjs/materials": "^8.0.1",
		"@babylonjs/post-processes": "^8.0.1",
		"@google/genai": "^0.7.0",
		"autotyping": "^1.2.6",
		"install": "^0.13.0",
		"preline": "^3.0.1",
		"replicate": "^1.0.1",
		"sse.js": "^2.6.0",
		"three": "^0.174.0",
		"three-custom-shader-material": "6.3.1",
		"three-mesh-bvh": "^0.9.0",
		"typewriter-effect": "^2.21.0"
	}
}
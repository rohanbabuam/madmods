		"supabase": "^2.22.12",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.7",
		"tailwindcss": "^4.1.5",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.0",
		"vite": "^6.3.5",
		"vitest": "^3.1.3",
		"wrangler": "^4.14.4"
